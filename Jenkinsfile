pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Hello we are in Build Stage'
                 echo 'Hello we are in Build Stage - 2'
            }
        }
        stage('Test') {
            steps {
                echo 'Hello we are in Build Stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Hello we are in Deploy Stage'
            }
        }
    }
}
